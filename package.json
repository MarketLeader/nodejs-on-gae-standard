{
  "name": "nodejs-on-gae-standard",
  "description": "A sample application of node.js on Google App Engine Standard environment",
  "version": "0.0.1",
  "private": false,
  "license": "Apache-2.0",
  "author": "rika-t@evaluni.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/rika-t/nodejs-on-gae-standard.git"
  },
  "scripts": {
    "build": "webpack --progress --mode production",
    "compile": "tsc",
    "deploy": "npm run build && gcloud app deploy --project=PROJECT_NAME --stop-previous-version --quiet",
    "start": "tsc && NODE_ENV=production node -r tsconfig-paths/register server/src/app.js",
    "start:local": "npm-run-all compile --parallel start:local:*",
    "start:local:client": "webpack --progress --mode development --watch",
    "start:local:compile": "tsc -w --preserveWatchOutput",
    "start:local:server": "nodemon -r tsconfig-paths/register server/src/app.js"
  },
  "engines": {
    "node": "8.x.x"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.16.0",
    "@types/node": "^8.10.21",
    "autoprefixer": "^9.0.0",
    "css-loader": "^1.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "mini-css-extract-plugin": "^0.4.1",
    "morgan": "^1.9.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.6",
    "rxjs": "^6.2.2",
    "sass-loader": "^7.0.3",
    "ts-loader": "^4.4.2",
    "tsconfig-paths": "^3.5.0",
    "typescript": "^2.9.2",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8"
  }
}
